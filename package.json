{
  "name": "@lisk-builders/lisk-hd-key",
  "version": "0.5.2",
  "description": "HD key derivation for Lisk coin",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc -p ./",
    "test": "jest"
  },
  "jest": {
    "verbose": true,
    "transform": {
      ".(ts|tsx)": "./node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/spec/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "globals": {
      "window": {},
      "ts-jest": {
        "tsConfigFile": "./tsconfig.json"
      }
    }
  },
  "keywords": [
    "ed25519",
    "bip32",
    "lisk",
    "lisk-js",
    "crypto",
    "hdkeys",
    "hdwallet"
  ],
  "author": "Aleksey Popov",
  "license": "GPL-3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lisk-builders/lisk-hd-key.git"
  },
  "bugs": {
    "url": "https://github.com/lisk-builders/lisk-hd-key/issues"
  },
  "homepage": "https://github.com/lisk-builders/lisk-hd-key",
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "jest-cli": "^22.0.0",
    "ts-jest": "^21.2.4",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "ed25519-hd-key": "^0.5.0",
    "lisk-js": "^0.5.0"
  }
}
